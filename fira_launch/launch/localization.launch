<?xml version="1.0" ?>
<launch> 
 <rosparam command="load" file="$(find self_localization)/config/localization.yaml" ns="FIRA"/>
 <rosparam command="load" file="$(find vision)/config/FIRA.yaml" ns="FIRA/vision"/>
 <rosparam command="load" file="$(find vision)/config/prosilica_driver.yaml" ns="prosilica_driver"/>
 <param name="FIRA/RobotNumber" value="1"/>
 <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch" />
 <node name="mjpeg_server" pkg="mjpeg_server" type="mjpeg_server"/>
 <node name="prosilica_driver" pkg="prosilica_camera" type="prosilica_node" respawn="true" respawn_delay="5" output="screen"/>
 <node name="Attack_motion" pkg="motion" type="Attack_motion_node" output="screen"/>
 <node name="white_line" pkg="vision" type="localization_white_line" output="screen"/>
 <node name="self_localization" pkg="self_localization" type="self_localization" output="screen"/>
 <node name="imu" pkg="imu_3d" type="imu_3d"/>
 <node name="monitor" pkg="vision" type="monitor" respawn="true" respawn_delay="5" />
</launch>
